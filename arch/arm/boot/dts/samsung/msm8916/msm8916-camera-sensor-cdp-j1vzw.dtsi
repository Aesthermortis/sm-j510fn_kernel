&soc {
   led_flash0: qcom,camera-led-flash {
        cell-index = <0>;
		compatible = "qcom,camera-led-flash";
		qcom,flash-type = <1>;
		flashen-gpio= <&msm_gpio 34 0x00>;
		flashtorch-gpio = <&msm_gpio 36 0x00>;
   };

	i2c_22: i2c@22 {
		cell-index = <22>;
		compatible = "i2c-gpio";
		i2c-gpio-scl = <&msm_gpio 30 0x00>;
		i2c-gpio-sda = <&msm_gpio 29 0x00>;
		i2c-gpio,delay-us = <5>;
